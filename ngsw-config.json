{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/media/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "9daa5f6f-ed5e-45d8-b705-6ca4bc142e58",
      "urls": ["https://run.mocky.io/v3/9daa5f6f-ed5e-45d8-b705-6ca4bc142e58"],
      "cacheConfig": {
        "maxAge": "6h",
        "maxSize": 10,  
        "strategy": "freshness",
        "timeout": "5s"
      }
    }
  ]
}
